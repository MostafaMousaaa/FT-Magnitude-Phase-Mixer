    <string notr="true">/* General QWidget Style */
QWidget {
    background-color: #2E3440;
    color: #D8DEE9;
    font-family: &quot;Segoe UI&quot;, Arial, sans-serif;
    font-size: 12pt;
}

/* PushButtons */
QPushButton {
    background-color: #4C566A;
    color: #ECEFF4;
    border: 1px solid #81A1C1;
    border-radius: 5px;
    padding: 5px 15px;
}
QPushButton:hover {
    background-color: #81A1C1;
}
QPushButton:pressed {
    background-color: #5E81AC;
}

/* SpinBoxes */
QSpinBox {
    background-color: #3B4252;
    color: #ECEFF4;
    border: 1px solid #81A1C1;
    border-radius: 3px;
    padding: 2px;
}
QSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 12px;
    border-left: 1px solid #81A1C1;
}
QSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 12px;
    border-left: 1px solid #81A1C1;
}

/* Horizontal Sliders */
QSlider::groove:horizontal {
    border: 1px solid #81A1C1;
    height: 5px;
    background: #3B4252;
}
QSlider::handle:horizontal {
    background: #81A1C1;
    border: 1px solid #5E81AC;
    width: 15px;
    margin: -5px 0;
    border-radius: 5px;
}

/* Labels */
QLabel {
    color: #ECEFF4;
    font-size: 12pt;
    font-weight: bold;
}

/* CheckBoxes */
QCheckBox {
    color: #ECEFF4;
}
QCheckBox::indicator {
    width: 15px;
    height: 15px;
}
QCheckBox::indicator:unchecked {
    border: 1px solid #81A1C1;
    background-color: #2E3440;
}
QCheckBox::indicator:checked {
    background-color: #81A1C1;
    border: 1px solid #5E81AC;
}

/* ComboBoxes */
QComboBox {
    background-color: #3B4252;
    color: #ECEFF4;
    border: 1px solid #81A1C1;
    border-radius: 3px;
    padding: 3px;
}
QComboBox QAbstractItemView {
    background-color: #3B4252;
    color: #ECEFF4;
    border: 1px solid #81A1C1;
}

/* LCD Number */
QLCDNumber {
    background-color: #3B4252;
    color: #81A1C1;
    border: 2px solid #81A1C1;
    border-radius: 5px;
}

/* QPolarChart */
QPolarChart {
    background: transparent;
}
QPolarChart &gt; .chart-title {
    color: #ECEFF4;
    font-size: 14pt;
    font-weight: bold;
}
QPolarChart &gt; .axis-title {
    color: #D8DEE9;
}

/* Scrollbars */
QScrollBar:horizontal, QScrollBar:vertical {
    border: none;
    background: #2E3440;
    width: 8px;
    margin: 0px;
}
QScrollBar::handle:horizontal, QScrollBar::handle:vertical {
    background: #4C566A;
    min-width: 20px;
    min-height: 20px;
}
QScrollBar::add-line, QScrollBar::sub-line {
    background: none;
    border: none;
}
